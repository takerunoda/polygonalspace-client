{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.tsx",
    "pages/all_posts.js",
    "pages/all_posts.js",
    "utils/getAllPosts.tsx",
    "context/UserContext.js",
    "pages/api/models/test_User.jser.js",
    "pages/api/models/SharedBookmark.js",
    "pages/api/models/Post.js",
    "Components/ArticleModal.tsxx",
    "pages/hubble_image/[id].js",
    "pages/NASA_HubbleModal.tsx",
    "pages/nasa_image/index.js",
    "pages/test.js",
    "pages/posts.js",
    "pages/bookmark-details/[_id].js",
    "utils/clickBack.js",
    "utils/updatePost.js",
    "pages/api/models/test_User.js",
    "pages/mybookmark_details.tsx",
    "pages/bookmark_ssg/[_id].js",
    "pages/_document.js",
    "pages/_app.jsx",
    "Components/RequestPasswordResetYesButtontsx"
  ],
  "exclude": ["node_modules"]
}
